<resources>
    <!-- Whether to switch dm for autoboot -->
    <bool name="dm_autoboot_setting_visible">true</bool>
    <bool name="config_searchapp_enabled">true</bool>
    <bool name="config_agps_enabled">true</bool>

    <!-- Whether enable Wifi/Bluetooth invalid in airplane mode -->
    <bool name="config_airplane_invalid">true</bool>

    <!-- Whether enable get network info -->
    <bool name="config_netinfo">true</bool>

    <!-- Whether to connect to wifi automatically -->
    <bool name="config_auto_connect_wifi_enabled">true</bool>

    <!-- Whether to switch cell to wifi -->
    <bool name="cell_to_wifi">true</bool>

    <!-- Whether to switch wifi to data -->
    <bool name="wifi_to_cell">true</bool>

    <!-- reselect wifi connection mode among ssid -->
    <bool name="config_wifi_connect_mode_among_ssid">true</bool>

    <!-- Whether to show hotspot via the ap's classification -->
    <bool name="set_wifi_priority">true</bool>

    <!-- Whether to show a preference item for mobile plan -->
    <bool name="config_show_mobile_plan">false</bool>

    <!-- Whether to show hotspot settings -->
    <bool name="show_wifi_hotspot_settings">true</bool>

    <!-- Whether to show abridged dialog for carrier AP -->
    <bool name="show_abridged_connecting_wifi_dialog">true</bool>

    <!-- Whether notify user that current connected ap is CMCC -->
    <bool name="connect_to_cmcc_notify">true</bool>

    <!-- Whether notify user that switch data service -->
    <bool name="confirm_to_switch_data_service">true</bool>

    <!-- Whether notify user in cell to wlan feature -->
    <bool name="cellular_to_wlan_hint">true</bool>

    <!-- Whether to enable data usage selection -->
    <bool name="config_data_usage_selection">true</bool>

    <!-- Whether to enable the default option to ipv4/v6 for creating new APN -->
    <bool name="config_default_apn_for_new">true</bool>

    <!-- Whether to hide ims apn -->
    <bool name="config_hide_ims_apns">true</bool>

    <!-- Whether to show carrier function in EAP method for adding network -->
    <bool name="config_eap_carrier_function">true</bool>

    <!-- Whether disable dds -->
    <bool name="config_dds_disable">true</bool>
</resources>
